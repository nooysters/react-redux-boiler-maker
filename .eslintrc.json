{
  "extends": "airbnb",
  "plugins": [
      "react",
      "jsx-a11y",
      "import"
  ],
  "globals": {
    "process": true,
    "window": true,
    "document": true
  },
  "rules": {
    "react/no-multi-comp": 0,
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "import/default": 0,
    "import/no-duplicates": 0,
    "import/named": 0,
    "import/namespace": 0,
    "import/no-unresolved": 0,
    "import/no-named-as-default": 2,
    "comma-dangle": 0,
    "indent": [2, 2, {"SwitchCase": 1}],
    "no-console": 0,
    "no-alert": 0,
    "react/prop-types": 0,
    "jsx-quotes": 0,
    "id-length": 0,
    "camelcase": 0,
    "no-return-assign": 0,
    "semi": 0,
    "spaced-comment": 0,
    "no-param-reassign": [
      2,
      {
        "props": true,
        "ignorePropertyModificationsFor": ["res", "resp"]
      }
    ]
  }
}
